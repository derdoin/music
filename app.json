{
  "name": "MisakiMusic",
  "logo": "https://telegra.ph/file/810ec0867be11dd1ed348.png",
  "description": "Telegram'ınızın Grup Sesli Sohbetinde şarkı çalmak için Açık Kaynak botu. MisakiDev tarafından desteklenmektedir.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/ByMisakiMey/MisakiMusicBOT",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your Music Player Bot Name.",
      "required": false,
      "value": "Misaki Müzik Bot"
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space) Get from @DingdiBot.",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "10"
    }
  }
}
